<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="infinitytracking_infinityjavascript.javascript_variable_extension.class">InfinityTracking\Bundle\InfinityJavascriptBundle\Twig\InfinityTrackingJavascriptVariableExtension.php</parameter>
    </parameters>

    <services>
        <service id="infinitytracking_infinityjavascript.javascript_variable_extension" class="%infinitytracking_infinityjavascript.javascript_variable_extension.class%">
            <argument>%infinitytracking_infinityjavascript.igrp%</argument>
            <argument>%infinitytracking_infinityjavascript.dgrps%</argument>
            <tag name="twig.extension" />
        </service>
    </services>

</container>
